{
  "name": "bilu",
  "version": "0.0.1",
  "description": "An app for buying experiences",
  "productName": "Bilu",
  "author": "Wingsoft",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint:fix": "eslint --ext .js,.ts,.vue ./ --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "ts:check": "tsc -p tsconfig.build.json --noEmit",
    "prepare": "husky install",
    "pre:commit": "lint-staged",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "dev:android": "quasar dev -m capacitor -T android",
    "build:android": "quasar build -m capacitor -T android",
    "apk:key": "keytool -genkey -v -keystore bilu-key.keystore -alias bilu_key -keyalg RSA -keysize 2048 -validity 20000",
    "apk:optimize": "$ANDROID_HOME/build-tools/30.0.2/zipalign -f -v 4 ./dist/capacitor/android/apk/release/app-release-unsigned.apk dist/capacitor/android/apk/release/bilu-app.apk",
    "apk:release": "$ANDROID_HOME/build-tools/30.0.2/apksigner sign --ks ./bilu-key.keystore --ks-key-alias dating_key ./dist/capacitor/android/apk/release/bilu-app.apk",
    "test:unit": "jest --no-cache --maxWorkers=1 --silent -c ./jest.config.js",
    "clj": "jest --clearCache"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "connect-history-api-fallback": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html-loader-jest": "^0.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "serve-static": "^1.15.0",
    "ts-jest": "^29.1.1",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "workbox-build": "^6.5.0",
    "workbox-cacheable-response": "^6.5.0",
    "workbox-core": "^6.5.0",
    "workbox-expiration": "^6.5.0",
    "workbox-precaching": "^6.5.0",
    "workbox-routing": "^6.5.0",
    "workbox-strategies": "^6.5.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
